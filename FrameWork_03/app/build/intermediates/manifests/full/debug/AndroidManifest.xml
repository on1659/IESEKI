<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="teamwarpcbstuido.framework_03"
    android:versionCode="4"
    android:versionName="1.2" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/player"
        android:label="IESEKI"
        android:largeHeap="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="teamwarpcbstuido.IESEKI.Layout.Loading"
            android:label="IESEKI"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="teamwarpcbstuido.IESEKI.Layout.Link"
            android:screenOrientation="portrait" />
        <activity
            android:name="teamwarpcbstuido.IESEKI.Layout.Help"
            android:screenOrientation="portrait" />
        <activity
            android:name="teamwarpcbstuido.IESEKI.Layout.Option"
            android:screenOrientation="portrait" />
        <activity
            android:name="teamwarpcbstuido.IESEKI.Layout.ZeroSetting"
            android:screenOrientation="portrait" />
        <activity
            android:name="teamwarpcbstuido.IESEKI.Layout.SelectMenu"
            android:label="@string/title_activity_loading"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>